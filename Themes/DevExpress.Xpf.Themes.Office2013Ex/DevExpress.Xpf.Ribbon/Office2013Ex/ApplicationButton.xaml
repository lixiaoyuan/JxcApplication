    <ControlTemplate x:Key="{dxrt:ApplicationButtonThemeKey ResourceKey=BorderTemplate2010}" TargetType="{x:Type ContentControl}">
        <Grid x:Name="Grid1" MinWidth="60" MinHeight="22">
            <Border x:Name="NormalState" Opacity="0" Background="#FF0072C6" cs:Name="Border_0006" />
