        <Grid x:Name="Grid1" Background="Transparent" cs:Name="Grid_0003">
            <Path x:Name="path" Stretch="Fill" Fill="#FF0072C6" Data="F1 M 8,0C 12.4183,0 16,3.58172 16,8C 16,12.4183 12.4183,16 8,16C 3.58172,16 -6.35783e-007,12.4183 -6.35783e-007,8C -6.35783e-007,3.58172 3.58172,0 8,0 Z M 2,8L 7,13L 10,13L 6,9L 13,9L 13,7L 5.8984,7L 10,3L 7,3L 2,8 Z " Width="16" Height="16" Margin="4,4,1,4" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Opacity="0.8" cs:Name="Path_0001" />
        </Grid>
