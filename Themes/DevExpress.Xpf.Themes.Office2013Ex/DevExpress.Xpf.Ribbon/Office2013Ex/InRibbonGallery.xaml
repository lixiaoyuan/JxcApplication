                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="DefaultState" Background="#FFFFFFFF" BorderBrush="#FFACACAC" BorderThickness="0,1,1,1" cs:Name="Border_0025" />
            <Border x:Name="HoverState" Background="#FFCDE6F7" BorderBrush="#FF92C0E0" BorderThickness="0,1,1,1" Opacity="0" cs:Name="Border_0027" />
            <Border x:Name="PressedState" Background="#FF92C0E0" BorderBrush="#FF92C0E0" BorderThickness="0,1,1,1" Opacity="0" cs:Name="Border_0029" />
            <Path x:Name="Glyph" Stretch="Fill" Fill="#FF3C3C3C" Data="F1 M 4,6L 3.03165e-013,2L 8,2L 4,6 Z M 0,9.53674e-007L 8,9.53674e-007L 8,1L 0,1L 0,9.53674e-007 Z " Width="8" Height="6" Margin="0,0,1,0" cs:Name="Path_0004" />
        </Grid>
    </ControlTemplate>  
    <!--END WPF-->
    <ControlTemplate x:Key="{dxrt:InRibbonGalleryThemeKey ResourceKey=BorderTemplate}" TargetType="{x:Type ContentControl}">
        <Grid Background="#FFACACAC" Margin="0,0,1,1" cs:Name="Grid_0005"> 
            <ContentPresenter />
        </Grid>
    </ControlTemplate>
</ResourceDictionary>