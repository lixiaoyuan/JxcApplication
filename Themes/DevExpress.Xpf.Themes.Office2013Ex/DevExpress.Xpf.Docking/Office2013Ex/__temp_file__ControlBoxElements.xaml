                            <Setter Property="MinHeight" TargetName="ControlBoxButtonBorder1" Value="44" />
                            <Setter Property="MinWidth" TargetName="ControlBoxButtonBorder1" Value="44" />
                        </Trigger>
                    </ControlTemplate.Triggers>
