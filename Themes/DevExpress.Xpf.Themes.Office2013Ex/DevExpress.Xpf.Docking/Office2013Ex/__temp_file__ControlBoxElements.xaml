                                        <ColorAnimation Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="UnpinnedGlyph" Duration="0" To="#FF666666" cs:Name="ColorAnimation_0010" />
                                    </Storyboard>
