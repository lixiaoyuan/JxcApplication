    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Docking.VisualElements.DocumentSelectorContainer}" TargetType="{x:Type ve:DocumentSelectorContainer}">
        <Style.Setters>
            <Setter Property="Foreground" Value="#FF3C3C3C" cs:Name="Setter_0001" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ve:DocumentSelectorContainer}">
                        <Grid>
                            <dx:DXBorder x:Name="Window" Background="#FFD6D6D6" cs:Name="Border_0026">
                                <dx:DXBorder Background="#FFFFFFFF" Margin="1" cs:Name="Border_0027">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <dx:DXBorder x:Name="Header" Background="#FFF0F0F0" Margin="1,1,1,0" cs:Name="Border_0028">
                                            <StackPanel Margin="12,8,12,8" VerticalAlignment="Center" HorizontalAlignment="Left" Orientation="Horizontal">
                                                <Image Margin="0,0,6,0" Stretch="None" VerticalAlignment="Top" HorizontalAlignment="Left" Source="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionImage}" />
                                                <StackPanel>
                                                    <TextBlock Foreground="#FF0072C6" TextWrapping="Wrap" VerticalAlignment="Bottom" HorizontalAlignment="Left" FontWeight="Light" FontSize="18" Text="{Binding Path=(local:DockLayoutManager.LayoutItem).Caption}" cs:Name="TextBlock_0001" />
                                                    <TextBlock TextWrapping="Wrap" HorizontalAlignment="Left" VerticalAlignment="Top" Text="{Binding Path=(local:DockLayoutManager.LayoutItem).Description}" />
                                                </StackPanel>
                                            </StackPanel>
                                        </dx:DXBorder>
                                        <dx:DXBorder x:Name="Content" Background="#FFFFFFFF" Grid.Row="1" cs:Name="Border_0029">
                                            <ContentPresenter x:Name="PART_ContainerContent" Content="{Binding}" Margin="12" />
                                        </dx:DXBorder>
                                        <dx:DXBorder x:Name="Footer" Background="#FFF0F0F0" Margin="1,0,1,1" MinHeight="24" Grid.Row="2" cs:Name="Border_0030">
                                            <ContentPresenter HorizontalAlignment="Left" VerticalAlignment="Center" Margin="12,0,0,0" Content="{Binding Path=(local:DockLayoutManager.LayoutItem).FooterDescription}" />
                                        </dx:DXBorder>
                                    </Grid>
                                </dx:DXBorder>
                            </dx:DXBorder>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>
    <ControlTemplate x:Key="{dxt:DocumentSelectorElementsThemeKey ResourceKey=ListBoxItem}" TargetType="{x:Type ListBoxItem}">
        <Grid>
            <Grid x:Name="HoverState" Opacity="0">
                <dx:DXBorder Background="#FFCDE6F7" Margin="0,0,0,1" cs:Name="Border_0031" />
            </Grid>
            <Grid x:Name="SelectedState" Opacity="0">
                <dx:DXBorder Background="#FF92C0E0" Margin="0,0,0,1" cs:Name="Border_0032" />
            </Grid>
            <dx:DXBorder Padding="{TemplateBinding Padding}">
                <ve:CaptionControl x:Name="caption" DataContext="{Binding}" Margin="3,2,3,2" />
            </dx:DXBorder>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Opacity" TargetName="SelectedState" Value="1" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Opacity" TargetName="HoverState" Value="1" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
