<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/charts/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:cs="colorscheme_ignorable_uri" mc:Ignorable="cs">
    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=Domain3DBrush}" Color="#FFD6D6D6" cs:Name="SolidColorBrush_0001" />
    <ControlTemplate x:Key="{dxt:ChartControlThemeKey ResourceKey=XYDiagram3DWrapperTemplate}" TargetType="ContentControl">
        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Padding="{TemplateBinding Padding}" Visibility="{TemplateBinding Visibility}">
            <ContentPresenter />
        </Border>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxt:ChartControlThemeKey ResourceKey=SimpleDiagram3DWrapperTemplate}" TargetType="ContentControl">
        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Padding="{TemplateBinding Padding}" Visibility="{TemplateBinding Visibility}">
            <ContentPresenter />
        </Border>
    </ControlTemplate>
</ResourceDictionary>