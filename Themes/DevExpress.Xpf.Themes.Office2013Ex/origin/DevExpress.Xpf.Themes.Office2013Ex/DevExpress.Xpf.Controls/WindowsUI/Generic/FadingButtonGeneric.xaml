        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Template" Value="{DynamicResource {dxwuit:WindowsUIElementsThemeKey ResourceKey=FadingButtonTemplate}}" />
    </Style>
</ResourceDictionary>