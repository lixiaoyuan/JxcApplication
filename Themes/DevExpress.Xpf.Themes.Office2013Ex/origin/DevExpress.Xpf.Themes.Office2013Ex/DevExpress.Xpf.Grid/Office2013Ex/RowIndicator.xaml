    <ControlTemplate x:Key="{dxgt:RowIndicatorThemeKey ResourceKey=RowPanelTemplate}" TargetType="{x:Type ContentControl}">
        <ContentPresenter />
    </ControlTemplate>
    <ControlTemplate x:Key="{dxgt:RowIndicatorThemeKey ResourceKey=RowOffsetControlTemplate}" TargetType="{x:Type dxg:RowOffset}">
        <Border x:Name="ContentBorder" Background="#FFFFFFFF" cs:Name="Border_0006">
            <Path x:Name="PART_OffsetPath" Stroke="#FFD6D6D6" StrokeThickness="1" cs:Name="Path_0008" />
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="ShowRowBreak" Value="True">
                <Setter Property="BorderBrush" TargetName="ContentBorder" Value="{StaticResource {dxgt:GridRowThemeKey ResourceKey=RowBreakBrush}}" />
                <Setter Property="BorderThickness" TargetName="ContentBorder" Value="{StaticResource {dxgt:GridRowThemeKey ResourceKey=RowBreakThickness}}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxgt:RowIndicatorThemeKey ResourceKey=GroupRowOffsetControlTemplate}" TargetType="{x:Type dxg:GroupRowOffset}">
        <Border x:Name="groupRowBorderFocused" Background="#FF92C0E0" cs:Name="Border_0005">
            <Border.Visibility>
                <Binding Path="SelectionState">
                    <Binding.Converter>
                        <dxg:SelectionStateToVisibilityConverter Value="Focused" />
                    </Binding.Converter>
                </Binding>
            </Border.Visibility>
        </Border>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxgt:RowIndicatorThemeKey ResourceKey=RowOffsetTemplate}" TargetType="{x:Type ContentControl}">
        <dxg:RowOffset AllowHorizontalLines="{Binding IsMasterRowExpanded, Converter={StaticResource BoolInverseConverter}}" AllowVerticalLines="False" BorderBrush="#00000000" Offset="{DynamicResource {dxgt:TableViewThemeKey ResourceKey=LeftGroupAreaIndent}}" ShowRowBreak="{Binding Path=ShowRowBreak}" RowLevel="{Binding Path=Level}" NextRowLevel="{Binding Path=NextRowLevel}" Template="{DynamicResource {dxgt:RowIndicatorThemeKey ResourceKey=RowOffsetControlTemplate}}" cs:Name="RowOffset_0001" />
    </ControlTemplate>
    <ControlTemplate x:Key="{dxgt:RowIndicatorThemeKey ResourceKey=GroupRowOffsetTemplate}" TargetType="{x:Type ContentControl}"></ControlTemplate>
</ResourceDictionary>