        <Border Background="{StaticResource RowIndicatorBackground}" BorderBrush="{StaticResource RowIndicatorBorderBrush}" BorderThickness="0,0,1,1" cs:Name="Border_0002">
            <ContentPresenter Content="{Binding DataContext, RelativeSource={RelativeSource TemplatedParent}}" ContentTemplate="{Binding Path=View.RowIndicatorContentTemplate}" />
        </Border>
    </DataTemplate>
    <DataTemplate x:Key="{dxgt:RowIndicatorThemeKey ResourceKey=GroupRowTemplate}">
        <Border Background="#FFFFFFFF" BorderBrush="#FFD6D6D6" BorderThickness="0,0,1,1" cs:Name="Border_0004">
            <ContentPresenter Content="{Binding DataContext, RelativeSource={RelativeSource TemplatedParent}}" ContentTemplate="{Binding Path=View.RowIndicatorContentTemplate}" />
        </Border>
    </DataTemplate>
