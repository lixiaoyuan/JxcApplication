                                                </dxb:BarEditItem.EditSettings>
                                            </dxb:BarEditItem>
                                        </dxr:RibbonPageGroup>
                                        <dxr:RibbonPageGroup Caption="{Binding ConverterParameter=RibbonPageGroup_TreeLayout, Converter={StaticResource DiagramStringIdConverter}}">
                                            <dxb:BarSubItem Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=TreeLayoutDirectionButtonStyle}}">
                                                <dxb:BarButtonItem Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=TreeLayoutLeftToRightButtonStyle}}" />
                                                <dxb:BarButtonItem Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=TreeLayoutRightToLeftButtonStyle}}" />
                                                <dxb:BarButtonItem Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=TreeLayoutTopToBottomButtonStyle}}" />
                                                <dxb:BarButtonItem Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=TreeLayoutBottomToTopButtonStyle}}" />
                                            </dxb:BarSubItem>
                                        </dxr:RibbonPageGroup>
                                    </dxr:RibbonPage>
                                </dxr:RibbonDefaultPageCategory>
                            </dxr:RibbonControl>
                            <dxr:RibbonStatusBarControl DockPanel.Dock="Bottom">
                                <dxr:RibbonStatusBarControl.RightItems>
                                    <dxb:BarEditItem Content="{TemplateBinding ZoomFactor, Converter={dxmvvm:FormatStringConverter FormatString=P0}}" EditValue="{Binding ZoomFactor, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type dxdiag:DiagramDesignerControl}}, Converter={dxdiag:NonlinearZoomConverter}, Mode=TwoWay}" EditWidth="100">
                                        <dxb:BarEditItem.EditSettings>
                                            <dxe:TrackBarEditSettings Minimum="0" Maximum="1" SmallStep="0.01" LargeStep="0.1" TickFrequency="0.5" />
                                        </dxb:BarEditItem.EditSettings>
                                    </dxb:BarEditItem>
                                </dxr:RibbonStatusBarControl.RightItems>
                                <dxr:RibbonStatusBarControl.LeftItems>
                                    <dxb:BarStaticItem RibbonStyle="SmallWithText">
                                        <dxb:BarStaticItem.Content>
                                            <MultiBinding Converter="{dxdiag:ItemPropertiesToStringConverter}">
                                                <Binding Path="SelectionToolsModel[Size]" RelativeSource="{RelativeSource FindAncestor, AncestorType={x:Type dxdiag:DiagramDesignerControl}}" />
                                                <Binding Path="SelectionToolsModel[Angle]" RelativeSource="{RelativeSource FindAncestor, AncestorType={x:Type dxdiag:DiagramDesignerControl}}" />
                                            </MultiBinding>
                                        </dxb:BarStaticItem.Content>
                                    </dxb:BarStaticItem>
                                </dxr:RibbonStatusBarControl.LeftItems>
                            </dxr:RibbonStatusBarControl>
                            <dxdo:DockLayoutManager DockPanel.Dock="Left">
                                <dxdo:LayoutGroup Orientation="Horizontal">
                                    <dxdo:LayoutPanel x:Name="PART_ToolboxPane" ItemWidth="250" MaxWidth="500" Style="{StaticResource DiagramLayoutPanelStyle}" AllowDock="True" AllowFloat="False" AllowContextMenu="False" AllowDockToDocumentGroup="False">
                                        <dxdo:LayoutPanel.CaptionTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding ConverterParameter=Panel_Shapes_Name, Converter={StaticResource DiagramStringIdConverter}}" Visibility="{Binding ElementName=CollapsedBehavior, Path=IsCompact, Converter={dxmvvm:BooleanToVisibilityConverter Inverse=True}}" />
                                            </DataTemplate>
                                        </dxdo:LayoutPanel.CaptionTemplate>
                                        <dxdo:LayoutPanel.ControlBoxContent>
                                            <ToggleButton Style="{StaticResource HideButtonStyle}" x:Name="CollapseButton" IsChecked="{Binding ElementName=CollapsedBehavior, Path=ButtonChecked, Mode=TwoWay}" />
                                        </dxdo:LayoutPanel.ControlBoxContent>
                                        <dxmvvm:Interaction.Behaviors>
                                            <dxdiag:PanelCollapsedBehavior x:Name="CollapsedBehavior" ButtonChecked="{Binding CollapsedButtonChecked, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type dxdiag:DiagramDesignerControl}}, Mode=TwoWay}" LayoutError="{Binding ElementName=Toolbox, Path=LayoutError}" />
                                        </dxmvvm:Interaction.Behaviors>
                                        <dxdiag:DiagramToolboxControl x:Name="Toolbox" DiagramControl="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type dxdiag:DiagramDesignerControl}}}" DefaultStencils="{Binding DefaultStencils, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type dxdiag:DiagramDesignerControl}}}" IsCompact="{Binding ElementName=CollapsedBehavior, Path=IsCompact}" ViewModel="{dxdiag:DiagramToolboxControlViewModel}" />
                                    </dxdo:LayoutPanel>
                                    <dxdo:DocumentPanel>
                                        <ScrollViewer Background="{DynamicResource {dxdiagt:DiagramControlThemeKeys ResourceKey=DiagramControlBackground}}" dx:ScrollBarExtensions.ScrollBarMode="TouchOverlap" CanContentScroll="True" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Name="ScrollViewer">
                                            <dxdiag:LayersHost x:Name="LayersHost" Focusable="True" FocusVisualStyle="{x:Null}" />
                                        </ScrollViewer>
                                    </dxdo:DocumentPanel>
                                    <dxdo:LayoutPanel x:Name="PART_PropertiesPane" AllowClose="True" Style="{StaticResource DiagramLayoutPanelStyle}" AllowHide="True" MinWidth="200" MaxWidth="500" ItemWidth="300" Caption="{Binding ConverterParameter=Panel_Properties_Name, Converter={StaticResource DiagramStringIdConverter}}">
                                        <dxpg:PropertyGridControl ScrollViewer.VerticalScrollBarVisibility="Auto" ShowCategories="False" SelectedObject="{Binding SelectionModel, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type dxdiag:DiagramDesignerControl}}}">
                                            <dxpg:PropertyGridControl.Resources>
                                                <DataTemplate x:Key="propertyGridComboboxEdit">
                                                    <dxe:ComboBoxEdit x:Name="PART_Editor" ItemsSource="{Binding StandardValues}" />
                                                </DataTemplate>
                                            </dxpg:PropertyGridControl.Resources>
                                        </dxpg:PropertyGridControl>
                                    </dxdo:LayoutPanel>
                                </dxdo:LayoutGroup>
                            </dxdo:DockLayoutManager>
                        </DockPanel>
                    </dxb:BarManager>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>