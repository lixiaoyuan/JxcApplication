                                        </dxr:RibbonPageGroup>
                                        <dxr:RibbonPageGroup Caption="{Binding ConverterParameter=RibbonPageGroup_Themes, Converter={StaticResource DiagramStringIdConverter}}">
                                            <dxr:RibbonGalleryBarItem Content="{Binding ConverterParameter=RibbonPageGroup_Themes, Converter={StaticResource DiagramStringIdConverter}}">
                                                <dxr:RibbonGalleryBarItem.Resources>
                                                    <Style TargetType="{x:Type dxb:Gallery}" x:Key="themeGalleryStyle">
                                                        <Setter Property="ItemGlyphBorderTemplate">
                                                            <Setter.Value>
