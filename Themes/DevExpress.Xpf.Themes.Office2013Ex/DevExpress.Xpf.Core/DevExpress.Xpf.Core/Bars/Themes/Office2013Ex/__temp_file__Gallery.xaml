<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" xmlns:dxbt="http://schemas.devexpress.com/winfx/2008/xaml/bars/themekeys" xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:cs="colorscheme_ignorable_uri" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core">
    <Thickness x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=DefaultItemMargin}">1,1,0,0</Thickness>
    <Thickness x:Key="{dxbt:GalleryItemGroupControlThemeKey ResourceKey=DefaultGroupCaptionMargin}">3,2,3,2</Thickness>
    <Thickness x:Key="{dxbt:GalleryItemGroupControlThemeKey ResourceKey=DefaultGroupItemsMargin}">0,0,1,1</Thickness>
    <Thickness x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=DefaultGlyphBorderPadding}">0</Thickness>
    <Thickness x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=DefaultGlyphMargin}">0</Thickness>
    <Thickness x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=DefaultCaptionMargin}">0</Thickness>
    <Thickness x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=DefaultDescriptionMargin}">0</Thickness>
    <Style x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=SplitLayoutStyle}" TargetType="{x:Type dxb:SplitLayoutPanel}">
        <Setter Property="SingleContent1Margin" Value="3" />
        <Setter Property="SingleContent2Margin" Value="3" />
        <Setter Property="TopContent1Margin" Value="3,3,3,0" />
        <Setter Property="TopContent2Margin" Value="3" />
        <Setter Property="BottomContent1Margin" Value="3,0,3,3" />
        <Setter Property="BottomContent2Margin" Value="3" />
        <Setter Property="LeftContent1Margin" Value="3,3,0,3" />
        <Setter Property="LeftContent2Margin" Value="3" />
        <Setter Property="RightContent1Margin" Value="0,3,3,3" />
        <Setter Property="RightContent2Margin" Value="3" />
        <Style.Triggers> 
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="SingleContent1Margin" Value="12" />
                <Setter Property="SingleContent2Margin" Value="12" />
                <Setter Property="TopContent1Margin" Value="12,12,12,0" />
                <Setter Property="TopContent2Margin" Value="12" />
                <Setter Property="BottomContent1Margin" Value="12,0,12,12" />
                <Setter Property="BottomContent2Margin" Value="12" />
                <Setter Property="LeftContent1Margin" Value="12,12,0,12" />
                <Setter Property="LeftContent2Margin" Value="12" />
                <Setter Property="RightContent1Margin" Value="0,12,12,12" />
                <Setter Property="RightContent2Margin" Value="12" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <dxb:StatedStyleSelector x:Key="{dxbt:GalleryControlThemeKey ResourceKey=StandaloneCaptionTextStyleSelector}">
        <dxb:StatedStyleSelectorState IsDefault="True">
            <Style TargetType="{x:Type ContentControl}">
                <Setter Property="Foreground" Value="#FF3C3C3C" cs:Name="Setter_0001" />
            </Style>
        </dxb:StatedStyleSelectorState>
        <dxb:StatedStyleSelectorState State="Checked">
            <Style TargetType="{x:Type ContentControl}">
                <Setter Property="Foreground" Value="#FF3C3C3C" cs:Name="Setter_0002" />
            </Style>
        </dxb:StatedStyleSelectorState>
        <dxb:StatedStyleSelectorState State="HoverChecked">
            <Style TargetType="{x:Type ContentControl}">
                <Setter Property="Foreground" Value="#FF3C3C3C" cs:Name="Setter_0003" />
            </Style>
        </dxb:StatedStyleSelectorState>
    </dxb:StatedStyleSelector>
    <dxb:StatedStyleSelector x:Key="{dxbt:GalleryControlThemeKey ResourceKey=StandaloneDescriptionTextStyleSelector}">
        <dxb:StatedStyleSelectorState IsDefault="True">
            <Style TargetType="{x:Type ContentControl}">
                <Setter Property="Foreground" Value="#FF3C3C3C" cs:Name="Setter_0004" />
                <Setter Property="Opacity" Value="0.75" />
            </Style>
        </dxb:StatedStyleSelectorState>
        <dxb:StatedStyleSelectorState State="Checked">
            <Style TargetType="{x:Type ContentControl}">
                <Setter Property="Foreground" Value="#FF3C3C3C" cs:Name="Setter_0005" />
                <Setter Property="Opacity" Value="0.75" />
            </Style>
        </dxb:StatedStyleSelectorState>
        <dxb:StatedStyleSelectorState State="HoverChecked">
            <Style TargetType="{x:Type ContentControl}">
                <Setter Property="Foreground" Value="#FF3C3C3C" cs:Name="Setter_0006" />
                <Setter Property="Opacity" Value="0.75" />
            </Style>
        </dxb:StatedStyleSelectorState>
    </dxb:StatedStyleSelector>
    <Style x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=DefaultItemCaptionTextStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Foreground" Value="#FF3C3C3C" cs:Name="Setter_0007" />
    </Style>
    <Style x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=DefaultItemDescriptionTextStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Foreground" Value="#FF666666" cs:Name="Setter_0008" />
    </Style>
    <Style x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=DefaultGroupCaptionTextStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Foreground" Value="#FF666666" cs:Name="Setter_0009" />
        <Setter Property="FontWeight" Value="Bold" />
    </Style>
    <Style x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=DefaultNormalFilterCaptionTextStyle}" TargetType="{x:Type ContentControl}">
        <Style.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Margin" Value="12" />
            </Trigger>
        </Style.Triggers>
        <Setter Property="Foreground" Value="#FF3C3C3C" cs:Name="Setter_0010" />
        <Setter Property="Margin" Value="4,3,4,4" />
        <Setter Property="FontWeight" Value="Bold" />
    </Style>
    <Style x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=DefaultSelectedFilterCaptionTextStyle}" TargetType="{x:Type ContentControl}">
        <Style.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Margin" Value="12" />
            </Trigger>
        </Style.Triggers>
        <Setter Property="Foreground" Value="#FF3C3C3C" cs:Name="Setter_0011" />
        <Setter Property="Margin" Value="4,3,4,4" />
        <Setter Property="FontWeight" Value="Bold" />
    </Style>
    <Style x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=DefaultHintTextStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Foreground" Value="#FF3C3C3C" cs:Name="Setter_0012" />
    </Style>
    <Style x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=DefaultHintCaptionTextStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Foreground" Value="#FF3C3C3C" cs:Name="Setter_0013" />
    </Style>
    <Style x:Key="{dxbt:GalleryControlThemeKey ResourceKey=StandaloneContentStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Margin" Value="1" />
        <Setter Property="Foreground" Value="#FFFFFFFF" cs:Name="Setter_0014" />
    </Style>
    <Style x:Key="{dxbt:GalleryControlThemeKey ResourceKey=StandaloneCaptionStyle}" TargetType="{x:Type ToggleButton}">
        <Setter Property="Margin" Value="0" />
        <Setter Property="Foreground" Value="#FFFFFFFF" cs:Name="Setter_0015" />
    </Style>
    <Style x:Key="{dxbt:GalleryControlThemeKey ResourceKey=StandaloneVerticalScrollBarContainerStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Margin" Value="0" />
    </Style>
    <Style x:Key="{dxbt:GalleryControlThemeKey ResourceKey=StandaloneGroupsBackgroundStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Margin" Value="0" />
    </Style>
    <Style x:Key="{dxbt:GalleryControlThemeKey ResourceKey=StandaloneGroupsViewportStyle}" TargetType="{x:Type dxb:GalleryGroupsViewer}">
        <Setter Property="Margin" Value="0" />
    </Style>
    <ControlTemplate x:Key="{dxbt:GalleryControlThemeKey ResourceKey=StandaloneCaptionTemplate}" TargetType="{x:Type ToggleButton}">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetName="Border" Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)" Duration="0" To="#FFCDE6F7" cs:Name="ColorAnimation_0001" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetName="Border" Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)" Duration="0" To="#FF92C0E0" cs:Name="ColorAnimation_0002" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="CheckedStates">
                    <VisualState x:Name="Unchecked" />
                    <VisualState x:Name="Checked">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetName="Border" Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)" Duration="0" To="#FF92C0E0" cs:Name="ColorAnimation_0003" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <dx:DXBorder x:Name="Border" Background="#FFB1D6F0" Margin="1" cs:Name="Border_0001">
                <ContentPresenter />
            </dx:DXBorder>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxbt:GalleryControlThemeKey ResourceKey=StandaloneBackgroundTemplate}" TargetType="{x:Type ContentControl}">
        <dx:DXBorder Background="#FFFFFFFF" BorderBrush="#FFB9B9B9" BorderThickness="1" cs:Name="Border_0003">
            <ContentPresenter />
        </dx:DXBorder>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxbt:GalleryControlThemeKey ResourceKey=StandaloneGroupsBackgroundTemplate}" TargetType="{x:Type ContentControl}">
        <ContentPresenter />
    </ControlTemplate>
    <ControlTemplate x:Key="StandaloneBorderTemplate_Normal" TargetType="{x:Type ContentControl}" />
    <ControlTemplate x:Key="StandaloneBorderTemplate_Hover" TargetType="{x:Type ContentControl}">
        <dx:DXBorder Background="#FFCDE6F7" cs:Name="Border_0004" />
    </ControlTemplate>
    <ControlTemplate x:Key="StandaloneBorderTemplate_HoverChecked" TargetType="{x:Type ContentControl}">
        <dx:DXBorder Background="#FFCDE6F7" cs:Name="Border_0005" />
    </ControlTemplate>
    <ControlTemplate x:Key="StandaloneBorderTemplate_Pressed" TargetType="{x:Type ContentControl}">
        <dx:DXBorder Background="#FF92C0E0" cs:Name="Border_0006" />
    </ControlTemplate>
    <ControlTemplate x:Key="StandaloneBorderTemplate_Checked" TargetType="{x:Type ContentControl}">
        <dx:DXBorder Background="#FFB1D6F0" cs:Name="Border_0007" />
    </ControlTemplate>
    <ControlTemplate x:Key="StandaloneBorderTemplate_Focused" TargetType="{x:Type ContentControl}">
        <dx:DXBorder Background="#FFCDE6F7" cs:Name="Border_0008" />
    </ControlTemplate>
    <ControlTemplate x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=StandaloneBorderTemplate}" TargetType="{x:Type dxb:ItemBorderControl}">
        <Grid Background="#00000000" cs:Name="Grid_0001">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="State">
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="Hover">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Hover" Storyboard.TargetProperty="Visibility" dxcn:ValueSetter.Visibility="Visible" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Pressed" Storyboard.TargetProperty="Visibility" dxcn:ValueSetter.Visibility="Visible" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Checked">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Checked" Storyboard.TargetProperty="Visibility" dxcn:ValueSetter.Visibility="Visible" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="HoverChecked">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Checked" Storyboard.TargetProperty="Visibility" dxcn:ValueSetter.Visibility="Visible" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Focused">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Focused" Storyboard.TargetProperty="Visibility" dxcn:ValueSetter.Visibility="Visible" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <ContentControl x:Name="PART_Normal" Template="{StaticResource StandaloneBorderTemplate_Normal}" Visibility="Visible" />
            <ContentControl x:Name="PART_Hover" Template="{StaticResource StandaloneBorderTemplate_Hover}" Visibility="Collapsed" />
            <ContentControl x:Name="PART_Pressed" Template="{StaticResource StandaloneBorderTemplate_Pressed}" Visibility="Collapsed" />
            <ContentControl x:Name="PART_HoverChecked" Template="{StaticResource StandaloneBorderTemplate_HoverChecked}" Visibility="Collapsed" />
            <ContentControl x:Name="PART_Checked" Template="{StaticResource StandaloneBorderTemplate_Checked}" Visibility="Collapsed" />
            <ContentControl x:Name="PART_Focused" Template="{StaticResource StandaloneBorderTemplate_Focused}" Visibility="Collapsed" />
            <ContentPresenter />
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=StandaloneGlyphBorderTemplate}" TargetType="{x:Type dxb:ItemBorderControl}">
        <Grid Background="#00000000" cs:Name="Grid_0002">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="State">
                    <VisualState x:Name="Normal">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Checked" Storyboard.TargetProperty="Visibility" dxcn:ValueSetter.Visibility="Collapsed" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Hover" />
                    <VisualState x:Name="Pressed" />
                    <VisualState x:Name="Checked">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Checked" Storyboard.TargetProperty="Visibility" dxcn:ValueSetter.Visibility="Visible" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="HoverChecked" />
                    <VisualState x:Name="Focused" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <ContentControl x:Name="PART_Checked" Template="{StaticResource StandaloneBorderTemplate_Checked}" Visibility="Collapsed" />
            <ContentPresenter />
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxbt:GalleryItemGroupControlThemeKey ResourceKey=CaptionTemplate}" TargetType="{x:Type ContentControl}">
        <dx:DXBorder x:Name="Border1" Background="#FFF0F0F0" MinHeight="24" Margin="1,0,1,0" cs:Name="Border_0009">
            <ContentPresenter x:Name="ContentPresenter1" Margin="2,0,0,0" />
        </dx:DXBorder>
        <ControlTemplate.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="MinHeight" TargetName="Border1" Value="43" />
                <Setter Property="Margin" TargetName="ContentPresenter1" Value="10" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxbt:GalleryItemGroupControlThemeKey ResourceKey=BackgroundTemplate}" TargetType="{x:Type ContentControl}">
        <ContentPresenter />
    </ControlTemplate>
    <ControlTemplate x:Key="{dxbt:GalleryItemGroupControlThemeKey ResourceKey=ItemsBackgroundTemplate}" TargetType="{x:Type ContentControl}">
        <ContentPresenter />
    </ControlTemplate>
    <ControlTemplate x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=HoverContentControlTemplate}" TargetType="{x:Type ContentControl}">
        <dx:DXBorder Background="#FFFFFFFF" BorderBrush="#FFB9B9B9" BorderThickness="1" cs:Name="Border_0011">
            <ContentPresenter Margin="3" />
        </dx:DXBorder>
    </ControlTemplate>
    
    

    <ControlTemplate x:Key="{dxbt:GalleryItemControlThemeKey ResourceKey=ItemToolTipBackgroundTemplate}" TargetType="{x:Type ContentControl}">
        <dx:DXBorder Background="#FFFFFFFF" BorderBrush="#FFB9B9B9" BorderThickness="1" Padding="2" cs:Name="Border_0013">
            <ContentPresenter />
        </dx:DXBorder>
    </ControlTemplate>
</ResourceDictionary>