<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys" xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:cs="colorscheme_ignorable_uri">
    <Style x:Key="{dxet:FilterPanelControlBaseThemeKey ResourceKey=FilterPanelBorderStyle}" TargetType="Border">
        <Setter Property="Background" Value="#FFFFFFFF" cs:Name="Setter_0001" />
        <Setter Property="BorderBrush" Value="#FFD6D6D6" cs:Name="Setter_0002" />
    </Style>
    <Style x:Key="{dxet:FilterPanelControlBaseThemeKey ResourceKey=FilterPanelEnableFilterStyle}" TargetType="{x:Type dxe:CheckEdit}">
        <Setter Property="Margin" Value="6,1,6,2" /> 
    </Style>
    <Style x:Key="{dxet:FilterPanelControlBaseThemeKey ResourceKey=FilterPanelTextStyle}" TargetType="{x:Type dxe:TextEdit}">
        <Setter Property="Margin" Value="0,2,0,2" /> 
    </Style>
    <ControlTemplate x:Key="{dxet:FilterPanelControlBaseThemeKey ResourceKey=ClearFilterButtonTemplate}" TargetType="{x:Type Button}">
        <Grid Background="#00000000" cs:Name="Grid_0001">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="path1" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <SolidColorBrush Color="#FF0072C6" cs:Name="SolidColorBrush_0001" />
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="path1" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <SolidColorBrush Color="#FF666666" cs:Name="SolidColorBrush_0002" />
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="path1" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <SolidColorBrush Color="#FFDDDDDD" cs:Name="SolidColorBrush_0003" />
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid x:Name="grid" Width="16" Height="16" HorizontalAlignment="Center" VerticalAlignment="Center">
                <Path x:Name="path1" Fill="#FF666666" Stretch="Fill" Data="F1M1212,770C1216.42,770 1220,773.582 1220,778 1220,782.418 1216.42,786 1212,786 1207.58,786 1204,782.418 1204,778 1204,773.582 1207.58,770 1212,770z" Width="16" Height="16" HorizontalAlignment="Center" VerticalAlignment="Center" cs:Name="Path_0001" />
                <Path x:Name="path2" Fill="#FFFFFFFF" Stretch="Fill" Data="F1M1218,775L1215,778 1218,781 1216,783 1213,780 1210,783 1208,781 1211,778 1208,775 1210,773 1213,776 1216,773 1218,775z" Width="10" Height="10" HorizontalAlignment="Center" VerticalAlignment="Center" cs:Name="Path_0002" />
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="dxc:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Width" TargetName="grid" Value="32" />
                <Setter Property="Height" TargetName="grid" Value="32" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxet:FilterPanelControlBaseThemeKey ResourceKey=FilterControlButtonTemplate}" TargetType="{x:Type Button}">
        <Grid Background="#00000000" cs:Name="Grid_0002">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="Glyph" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <SolidColorBrush Color="#FF0072C6" cs:Name="SolidColorBrush_0004" />
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="Glyph" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <SolidColorBrush Color="#FF666666" cs:Name="SolidColorBrush_0005" />
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="Glyph" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <SolidColorBrush Color="#FFDDDDDD" cs:Name="SolidColorBrush_0006" />
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid x:Name="grid" HorizontalAlignment="Center" VerticalAlignment="Center" Width="16" Height="16">
                <Path x:Name="Glyph" Stretch="Fill" Fill="#FF666666" Data="F1 M 640,394C 643,394 645,396 645,399L 636,408L 631,408L 631,403L 640,394 Z M 642,400C 642,399.713 641.946,399.426 641.849,399.151L 635.846,405.154C 635.945,405.43 636,405.716 636,406L 642,400 Z M 640.517,397.483L 634.513,403.487C 634.907,403.748 635.253,404.095 635.511,404.489L 641.517,398.483C 641.259,398.088 640.912,397.741 640.517,397.483 Z M 639,397L 633,403C 633.287,403 633.573,403.054 633.848,403.152L 639.85,397.15C 639.574,397.053 639.288,397 639,397 Z M 635,407C 635,405.49 633.49,404 632,404L 632,407L 635,407 Z " Width="14" Height="14" HorizontalAlignment="Center" VerticalAlignment="Center" cs:Name="Path_0003" />
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="dxc:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="Width" TargetName="grid" Value="32" />
                <Setter Property="Height" TargetName="grid" Value="32" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxet:FilterPanelControlBaseThemeKey ResourceKey=MRUComboBoxTemplate}">
        <dxc:DXDockPanel>
            <Path dxc:DXDockPanel.Dock="Right" Stretch="Fill" Fill="{TemplateBinding Foreground}" Data="F1M1284,845L1281,848 1278,845 1284,845z" Width="6" Height="3" Margin="3,2,3,0">
                <Path.Visibility>
                    <Binding Path="(dxe:BaseEdit.OwnerEdit).ShowEditorButtons" RelativeSource="{RelativeSource TemplatedParent}">
                        <Binding.Converter>
                            <dxc:BooleanToVisibilityConverter />
                        </Binding.Converter>
                    </Binding>
                </Path.Visibility>
            </Path>
            <TextBlock Text="{Binding Path=(dxe:BaseEdit.OwnerEdit).EditValue, RelativeSource={RelativeSource TemplatedParent}}" TextTrimming="WordEllipsis" TextDecorations="{Binding Path=(dxe:BaseEdit.OwnerEdit).TextDecorations, RelativeSource={RelativeSource TemplatedParent}}" Foreground="#FF3C3C3C" cs:Name="TextBlock_0001" />
        </dxc:DXDockPanel>
    </ControlTemplate>
    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Editors.Filtering.FilterPanelCaptionControl}" TargetType="dxe:FilterPanelCaptionControl">
        <Setter Property="Foreground" Value="#FF3C3C3C" cs:Name="Setter_0003" />
        <Setter Property="FontSize" Value="11" />
        <Setter Property="Margin" Value="6,0,6,0" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dxe:FilterPanelCaptionControl">
                    <ContentPresenter />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>