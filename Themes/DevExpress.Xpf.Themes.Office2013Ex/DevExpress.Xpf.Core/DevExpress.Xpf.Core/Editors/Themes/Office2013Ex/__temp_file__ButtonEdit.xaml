                <ContentPresenter x:Name="Glyph" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.ColumnSpan="2" ContentTemplate="{TemplateBinding ContentTemplate}">
                    <ContentPresenter.Content>
                        <SolidColorBrush Color="{DynamicResource {dxet:SpinEditThemeKey ResourceKey=ArrowBrushColor}}" />
                    </ContentPresenter.Content>
                </ContentPresenter>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="MinWidth" TargetName="Grid1" Value="44" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
</ResourceDictionary>